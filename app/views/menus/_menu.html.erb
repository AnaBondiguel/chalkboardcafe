<div id="<%= dom_id menu %>">
  <p>
    <strong>Name:</strong>
    <%= menu.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= menu.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= menu.price %>
  </p>

  <p>
    <strong>Availability:</strong>
    <%= menu.availability %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= menu.category %>
  </p>

  <p>
    <strong>User:</strong>
    <%= menu.user %>
  </p>

  <p>
    <strong>References:</strong>
    <%= menu.references %>
  </p>

</div>
